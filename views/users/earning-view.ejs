<body>
  <%- include('../templates/header.ejs'); %>
  <%- include('../templates/navbar.ejs'); %>

  <main id="main" class="main">

    <div class="pagetitle">
      <h1>View Statistics</h1>
      <nav>
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="/admin">Home</a></li>
          <li class="breadcrumb-item active">View Statistics</li>
        </ol>
      </nav>
    </div><!-- End Page Title -->

    <section class="section profile">
      <div class="row">

        <div class="col-xl-12 col-md-12">

          <div class>
            <div class>

              <table class="table w-90 col-md-12 col-xs-3" id="tblUser">
                <thead>

                  <tr>
                    <th>Order Id</th>
                    <th>Customer Name</th>
                    <th>Amount</th>
                    <th>Earnings</th>
                  </tr>
                </thead>
                <tbody>
                  <!-- loop start -->
                  <% var earnings = 0; %>
                  
                  <% inforders.forEach(o => { %>
                    <% if (!o.refund) { %>
                     
                    
                    <% var etot = 0; %>
                <% etot = Number(o.amount) - 50; %>
                <% etot = Number(etot/100)*(Number(o.result[0].discount)/2) %>
                <% earnings = earnings + etot %>
                  <tr>
                    <td><%= o._id %></td>
                    <td><%= o.user[0].name %></td>
                    <td>Rs. <%= o.amount.toFixed(2)%></td>
                    <td>Rs. <%= etot.toFixed(2) %></td>
                  </tr>
                  <% }}) %>

                </tbody>

              </table>
              <!-- ============displaying data with approval button========== -->
              
            </div>
          </div>

        </div>
      </div>
      <div class="container my-5 fw-bold text-center">
        <p>The Total estimated Earnings is: â‚¹ <span style="color: brown;"><%= earnings.toFixed(2) %></span></p>
      </div>
    </section>

  </main><!-- End #main -->

